{
  "name": "snodejs",
  "version": "2.0.2",
  "description": "SNodeJs is a Node.js application designed to demonstrate secure coding practices in web development. This project serves as a foundation for building secure, scalable web applications using Node.js.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env TZ=Asia/Kolkata APP_ENV=production pm2 start index.js",
    "dev": "cross-env TZ=Asia/Kolkata APP_ENV=development nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo:all": "node system/warnings/migrate-undo-warning.js && npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo:all": "node system/warnings/seed-undo-warning.js && npx sequelize-cli db:seed:undo:all",
    "fresh:migrate": "node system/warnings/fresh-warning.js && npm run fresh:migrate",
    "fresh:all": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "model:generate": "npx sequelize-cli model:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beraujjwal/snode.git"
  },
  "bugs": {
    "url": "https://github.com/beraujjwal/snode/issues"
  },
  "keywords": [
    "api",
    "AWS",
    "boilerplate",
